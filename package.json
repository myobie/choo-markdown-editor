{
  "name": "choo-markdown-editor",
  "version": "1.0.0",
  "description": "An example of a contenteditable markdown editor using choo",
  "scripts": {
    "start": "budo example.js -v -l -o -- -t babelify -t nanohtml -t sheetify",
    "build": "browserify example.js -o bundle.js -t babelify -t nanohtml -t sheetify"
  },
  "author": "Nathan Herald <me@nathanherald.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "@babel/plugin-transform-arrow-functions": "^7.0.0-beta.51",
    "@babel/plugin-transform-destructuring": "^7.0.0-beta.51",
    "@babel/plugin-transform-for-of": "^7.0.0-beta.51",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.51",
    "@babel/plugin-transform-spread": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babelify": "^9.0.0",
    "budo": "^11.3.2",
    "choo-devtools": "^2.5.1",
    "nanohtml": "^1.2.4",
    "sheetify": "^7.3.3",
    "tachyons": "^4.10.0",
    "tinyify": "^2.4.3"
  },
  "dependencies": {
    "choo": "^6.12.1"
  }
}
